{
  "name": "drupal/radix",
  "description": "Radix is a base theme for Drupal. It has Bootstrap, Sass, BrowserSync and Font Awesome built-in.",
  "type": "drupal-theme",
  "homepage": "https://www.drupal.org/project/radix",
  "authors": [
    {
      "name": "Arshad Chummun",
      "email": "arshad@chummun.com",
      "homepage": "https://arshad.io",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/radix"
  },
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "require": {
    "drupal/components": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Drush\\Commands\\radix\\": "Commands/radix/"
    }
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^9"
      }
    }
  }
